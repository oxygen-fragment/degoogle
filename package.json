{
  "name": "degoogle",
  "version": "2.0.0-alpha.1",
  "description": "Modern TypeScript CLI for extracting direct Google result URLs",
  "license": "MIT",
  "type": "commonjs",
  "bin": {
    "degoogle-ts": "dist/cli.js"
  },
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "typecheck": "tsc --noEmit -p tsconfig.json",
    "degoogle": "node dist/cli.js",
    "test:precode": "node --test tests/precode/*.test.mjs"
  },
  "engines": {
    "node": ">=18.17"
  },
  "devDependencies": {
    "@types/node": "^22.13.10",
    "typescript": "^5.8.2"
  }
}
